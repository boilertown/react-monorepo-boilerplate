{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"moduleResolution": "node",
		"skipLibCheck": true,
		"isolatedModules": true,
		"useUnknownInCatchVariables": false,
		"allowJs": true,
		"noPropertyAccessFromIndexSignature": false
	}
}
